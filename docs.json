{
  "intro": "Corrected and optimized Mintlify configuration",
  "$schema": "https://mintlify.com/schema.json",
  "$schema": "https://mintlify.com/schema.json",
  "name": "Houseparty",
  "favicon": "/favicon.svg",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "colors": {
    "primary": "#F7F9FA",
    "light": "#F7F9FA",
    "dark": "#F7F9FA"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "pages": [
          {
            "title": "Introduction",
            "path": "/"
          }
        ]
      },
      {
        "tab": "Docs",
        "pages": [
          {
            "title": "Getting Started",
            "path": "/getting-started"
          },
          {
            "title": "Essentials",
            "pages": [
              { "title": "Code", "path": "/essentials/code" },
              { "title": "Images", "path": "/essentials/images" },
              { "title": "Settings", "path": "/essentials/settings" },
              { "title": "Navigation", "path": "/essentials/navigation" },
              { "title": "Reusable Snippets", "path": "/essentials/reusable-snippets" }
            ]
          }
        ]
      }
    },
  "navigation": {
    "tabs": [
      {
        "title": "Home",
        "path": "/"
      },
      {
        "title": "Docs",
        "path": "/docs"
      }
    ],
    "global": {
      "anchors": [
        {
          "title": "Documentation",
          "href": "https://mintlify.com/docs"
        },
        {
          "title": "Blog",
          "href": "https://mintlify.com/blog"
        },
        {
          "title": "GitHub",
          "path": "https://github.com/mintlify"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Documentation",
        "url": "/docs"
      },
      {
        "label": "Blog",
        "url": "https://mintlify.com/blog"
      }
    ],
    "socials": {
      "telegram": "https://t.me/",
      "x": "https://x.com/",
      "github": "https://github.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  },
  "favicon": "/favicon.svg"
}
